{
  "name": "fitrition",
  "version": "0.0.0",
  "private": true,
  "publisher": "Fitrition LLC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fitrition/fitrition.git"
  },
  "bugs": {
    "url": "https://github.com/Fitrition/fitrition/issues"
  },
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --aot --prod && yarn build:server",
    "build:server": "tsc server.ts --outDir dist/",
    "ci": "yarn lint:all && yarn test:all && yarn e2e:all",
    "dev": "ng serve",
    "e2e": "ng e2e",
    "e2e:all": "yarn e2e:fts:headless",
    "e2e:fts": "yarn e2e",
    "e2e:fts:headless": "yarn e2e:fts --protractor-config='projects/fitrition-site/e2e/ci.protractor.conf.js'",
    "lint": "ng lint",
    "lint:all": "yarn lint",
    "ng": "ng",
    "release": "standard-version",
    "start": "node dist/server.js",
    "test": "yarn test:headless",
    "test:all": "yarn test",
    "test:chrome": "ng test --code-coverage --browsers=Chrome --watch=false",
    "test:chrome:headless": "ng test --code-coverage --browsers=ChromeHeadless --watch=false",
    "test:chrome:headless:watch": "ng test --code-coverage --browsers=ChromeHeadless --watch=true",
    "test:chrome:watch": "ng test --code-coverage --browsers=Chrome --watch=true",
    "test:firefox": "ng test --code-coverage --browsers=Firefox --watch=false",
    "test:firefox:headless": "ng test --code-coverage --browsers=FirefoxHeadless --watch=false",
    "test:firefox:headless:watch": "ng test --code-coverage --browsers=FirefoxHeadless --watch=true",
    "test:firefox:watch": "ng test --code-coverage --browsers=Firefox --watch=true",
    "test:headless": "yarn test:chrome:headless",
    "test:headless:watch": "yarn test:chrome:headless:watch",
    "test:watch": "yarn test:chrome:watch"
  },
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/cdk": "11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/material": "11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "express": "^4.17.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@commitlint/config-angular": "^12.0.1",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "commitizen": "^4.2.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^5.1.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "standard-version": "^9.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "maxHeaderWidth": 72,
      "maxLineWidth": 80
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --cache --fix",
    "*.{scss}": "stylelint --fix",
    "*.{ts,html,scss,json,md}": "prettier --write"
  }
}
